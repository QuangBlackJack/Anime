<div>

    <h1 style="margin-left: 10px;">ZERO TWO</h1>

    <div style="float: right; margin-top: 20px; margin-right: 10px" class="rounded bg-dark">
        <img width="400px" src="https://emoji.gg/assets/emoji/1409-zerotwo-dance.gif" class="rounded float-right"
            alt="...">
    </div>

    <div style="width: 57rem; margin-left: 10px" class="mt-4">
        <div class="row">

            {{#each Movie}}
            <div style="margin-top: 10px;" class="col-sm-6 col-lg-4">
                <div class="card">
                    <img class="card-img-top" src="{{this.image}}" alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title">{{this.name}}</h5>
                        <p class="card-text">Some quick example text to build on the card title and make up the bulk of
                            the
                            card's content.</p>
                        <a href="watch/{{this.slug}}" class="btn btn-primary">Go somewhere</a>
                    </div>
                </div>
            </div>
            {{/each}}

        </div>
    </div>

</div>